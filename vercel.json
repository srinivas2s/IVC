{
    "version": 2,
    "cleanUrls": true,
    "functions": {
        "server/index.js": {
            "maxDuration": 15
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}